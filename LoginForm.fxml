<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="265.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="userNameField" layoutX="216.0" layoutY="118.0" />
      <Label fx:id="userName" layoutX="34.0" layoutY="120.0" text="Username :">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="password" layoutX="29.0" layoutY="177.0" text="Password:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="instruct" layoutX="29.0" layoutY="48.0" text="Please enter your credentials">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="216.0" layoutY="175.0" />
      <Button fx:id="btnLogin" defaultButton="true" layoutX="315.0" layoutY="221.0" mnemonicParsing="false" onMouseClicked="#checkLogIn" text="Log In" />
      
   </children>
</AnchorPane>
